{
  "version": "0.0.1",
  "name": "jest-applause-reporter",
  "umd:name": "jest-applause-reporter",
  "repository": "https://github.com/ApplauseOSS/jest-applause-reporter",
  "description": "Jest results reporter for Applause Automation",
  "unpkg": "dist/index.min.js",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "clean": "rm -rf ./dist/",
    "lint": "eslint ./src/ ./test/ --fix",
    "lint-verify": "eslint ./src/ ./test/",
    "build": "rollup -c",
    "test": "yarn jest --detectOpenHandles",
    "publish-verify": "npm publish --dry-run",
    "all": "yarn install && yarn lint && yarn build && yarn test && yarn publish-verify"
  },
  "keywords": [
    "Applause",
    "jest",
    "reporter"
  ],
  "devDependencies": {
    "@rollup/plugin-node-resolve": "13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@tsconfig/node14": "^1.0.1",
    "@types/jest": "^29.5.4",
    "@types/node": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "rollup": "2.56.3",
    "rollup-plugin-dts": "^4.0.0",
    "rollup-plugin-terser": "7.0.2",
    "ts-node": "10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "yarn": "^1.22.11"
  },
  "dependencies": {
    "@jest/reporters": "^29.6.4",
    "@jest/test-result": "^29.6.4",
    "@jest/types": "^29.6.3",
    "auto-api-client-js": "https://github.com/ApplauseOSS/applause-reporter-common#master",
    "axios": "^0.21.4",
    "jest": "^29.6.4"
  }
}
